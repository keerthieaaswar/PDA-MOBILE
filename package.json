    "react-native-video-player": "0.10.0",
    "react-native-webview": "3.2.1",
    "react-redux": "6.0.1",
    "redux": "4.0.1",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "rn-fetch-blob": "0.10.15"
