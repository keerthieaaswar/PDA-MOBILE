    "react-test-renderer": "16.8.3",
    "redux-mock-store": "1.5.3"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-navigation|react-native-i18n)"
    ],
    "setupFiles": [
      "<rootDir>/setupTest.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/?(*.)(spec|test).js"
    ]
  },
  "rnpm": {
    "assets": [
      "src/assets/fonts"
    ]
  },
  "pre-commit": [
    "test"
  ]
